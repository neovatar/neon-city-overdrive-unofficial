{
  "name": "neon-city-overdrive-unofficial",
  "description": "CSS compiler for the ncou system",
  "scripts": {
    "build": "npx sass src/scss/neon-city-overdrive-unofficial.scss css/neon-city-overdrive-unofficial.css --style=expanded --no-source-map",
    "watch": "npx sass src/scss/neon-city-overdrive-unofficial.scss css/neon-city-overdrive-unofficial.css --style=expanded --source-map --watch"
  },
  "devDependencies": {
    "sass": "^1.53.0"
  },
  "volta": {
    "node": "20.11.1",
    "npm": "10.8.2"
  }
}
